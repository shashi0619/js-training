// wap to find max num by if else 

const readlineSync = require ("readline-sync");

const number1 = readlineSync.question (" number 1 :")

const number2 = readlineSync.question("number 2 :")


if (number1>=number2)
{
    console.log(number1 +" from number 1  is maximum ")
}

else
{
    console.log(number2 + "from number 2 is maximum " )
}

console.log (" end of if else ")




